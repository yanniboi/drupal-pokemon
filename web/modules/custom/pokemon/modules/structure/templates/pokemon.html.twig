{#
/**
 * @file pokemon.html.twig
 * Default theme implementation to present Pokemon data.
 *
 * This template is used when viewing Pokemon pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_pokemon()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('pokemon') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
